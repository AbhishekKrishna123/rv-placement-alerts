<body>
    <nav class="cyan darken-1">
        <div class="nav-wrapper">
            <a href="#" class="brand-logo center">RV Placement Alerts</a>
        </div>
    </nav>

    <div class="container content">
        <h4>IEM Placed Students</h4>

        {{#each students}}
